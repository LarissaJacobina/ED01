#include<stdio.h>
#include<stdlib.h>
#define TAMANHO 27

int main(void){

char aux;
char palavra[TAMANHO];	  
	    printf("Digite uma palavra de no m√°ximo 20 caracteres: \n");
		scanf("%s",palavra);
		
		printf("\n");
        printf("%s\t", palavra);
		printf("\n");

    for (int i = 0; i < TAMANHO; i++) {
        for (int j = 0; j < TAMANHO - 1; j++) {
            if (palavra[j] > palavra[j + 1]) {
                aux = palavra[j];
                palavra[j] = palavra[j + 1];
                palavra[j + 1] = aux;
            }
        }
        
    }
	for (int i = 0; i < TAMANHO; i++) {
    printf("%c\t", palavra[i]);
	}   
    
		
}
